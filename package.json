{
  "scripts": {
    "start": "node .",
    "dev": "nodemon .",
    "deploy": "git push clever HEAD:master"
  },
  "main": "index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-slow-down": "^1.3.1",
    "morgan": "^1.9.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
